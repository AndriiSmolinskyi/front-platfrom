\u0440\\u043E\\u0431\\u043E\\u0442\\u0438 \\u043D\\u0430\\u0434 \\u0437\\u0430\\u0432\\u0434\\u0430\\u043D\\u043D\\u044F\\u043C \\u0432\\u0438 \\u043C\\u043E\\u0436\\u0435\\u0442\\u0435 \\u043E\\u0431'\\u0454\\u0434\\u043D\\u0430\\u0442\\u0438 (merge) \\u0433\\u0456\\u043B\\u043A\\u0443 \\u0437 \\u043E\\u0441\\u043D\\u043E\\u0432\\u043D\\u043E\\u044E \\u0433\\u0456\\u043B\\u043A\\u043E\\u044E, \\u0449\\u043E\\u0431 \\u0434\\u043E\\u0434\\u0430\\u0442\\u0438 \\u0432\\u0430\\u0448\\u0456 \\u0437\\u043C\\u0456\\u043D\\u0438 \\u0434\\u043E \\u043F\\u0440\\u043E\\u0435\\u043A\\u0442\\u0443.\"\n          }, void 0, false, {\n            fileName: _jsxFileName,\n            lineNumber: 46,\n            columnNumber: 25\n          }, this), /*#__PURE__*/_jsxDEV(\"p\", {\n            className: \"content__paragraph\",\n            children: \"\\u0426\\u0456 \\u0442\\u0440\\u0438 \\u043E\\u0441\\u043D\\u043E\\u0432\\u043D\\u0456 \\u0442\\u0435\\u0440\\u043C\\u0456\\u043D\\u0438 \\u0434\\u043E\\u043F\\u043E\\u043C\\u0430\\u0433\\u0430\\u044E\\u0442\\u044C \\u043E\\u0440\\u0433\\u0430\\u043D\\u0456\\u0437\\u0443\\u0432\\u0430\\u0442\\u0438 \\u0440\\u043E\\u0431\\u043E\\u0442\\u0443 \\u0437 \\u043A\\u043E\\u0434\\u043E\\u043C \\u0443 Git \\u0442\\u0430 \\u0441\\u043F\\u0440\\u043E\\u0449\\u0443\\u044E\\u0442\\u044C \\u0441\\u043F\\u0456\\u043B\\u044C\\u043D\\u0443 \\u0440\\u043E\\u0431\\u043E\\u0442\\u0443 \\u043D\\u0430\\u0434 \\u043F\\u0440\\u043E\\u0435\\u043A\\u0442\\u0430\\u043C\\u0438.\"\n          }, void 0, false, {\n            fileName: _jsxFileName,\n            lineNumber: 47,\n            columnNumber: 25\n          }, this)]\n        }, void 0, true, {\n          fileName: _jsxFileName,\n          lineNumber: 43,\n          columnNumber: 21\n        }, this)]\n      }, void 0, true, {\n        fileName: _jsxFileName,\n        lineNumber: 41,\n        columnNumber: 17\n      }, this), /*#__PURE__*/_jsxDEV(\"div\", {\n        className: \"content\",\n        children